{
  "name": "node-rust-pty",
  "package": {
    "name": "@ylayerdynamics/node-rust-pty",
    "version": "0.0.0",
    "description": "A Node.js native addon for PTY functionality using Rust",
    "repository": {
      "type": "git",
      "url": "https://github.com/layerdynamics/node-rust-pty.git"
    },
    "license": "MIT",
    "os": [
      "darwin",
      "linux",
      "win32"
    ],
    "cpu": [
      "x64",
      "arm64"
    ]
  },
  "triples": {
    "defaults": true,
    "additional": [
      "aarch64-apple-darwin",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc"
    ]
  },
  "features": {},
  "build": {
    "args": [
      "--release"
    ],
    "features": []
  },
  "targetDir": "target",
  "clean": true,
  "copyTo": {
    "npm": "artifacts/"
  },
  "scripts": {
    "prepack": "npm run build",
    "postbuild": "node scripts/postbuild.js"
  },
  "artifacts": {
    "directories": [
      "npm"
    ],
    "includeFilePatterns": [
      "*.node"
    ],
    "excludeFilePatterns": []
  },
  "testCommand": "npm run test",
  "npmPublishArgs": [
    "--access",
    "public"
  ],
  "dtsDirectories": [
    "index.d.ts"
  ],
  "binaryName": "node-rust-pty",
  "cargoLock": "Cargo.lock",
  "crateVersion": true,
  "optionalDependencies": {
    "node-rust-pty-win32-x64-msvc": "undefined",
    "node-rust-pty-darwin-x64": "undefined",
    "node-rust-pty-linux-x64-gnu": "undefined"
  }
}