{
	"name": "node-rust-pty",
	"version": "0.1.0",
	"main": "index.js",
	"types": "index.d.ts",
	"napi": {
		"name": "node-rust-pty",
		"triples": {
			"defaults": true,
			"additional": [
				"aarch64-apple-darwin"
			]
		}
	},
	"license": "MIT",
	"devDependencies": {
		"@napi-rs/cli": "^2.18.4",
		"ava": "^6.0.1",
		"pino": "^9.4.0"
	},
	"ava": {
		"timeout": "3m"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"artifacts": "napi artifacts --config napi-rs-config.json",
		"build": "napi build --platform --release --config napi-rs-config.json",
		"build:debug": "napi build --platform --config napi-rs-config.json",
		"prepublishOnly": "napi prepublish -t npm --skip-gh-release --config napi-rs-config.json",
		"test": "ava --serial",
		"universal": "napi universal --config napi-rs-config.json",
		"version": "napi version --config napi-rs-config.json"
	},
	"optionalDependencies": {
		"node-rust-pty-darwin-arm64": "0.1.0",
		"node-rust-pty-darwin-x64": "0.1.0",
		"node-rust-pty-linux-x64-gnu": "0.1.0",
		"node-rust-pty-win32-x64-msvc": "0.1.0"
	}
}
